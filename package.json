{
  "name": "FreshFoodMHI",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage && open coverage/lcov-report/index.html || xdg-open coverage/lcov-report/index.html",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "android:run6": "$ANDROID_HOME/tools/emulator -avd Nexus_5_API_27 & react-native run-android",
    "android:run5": "$ANDROID_HOME/tools/emulator -avd Pixel_5inch_API_24 & react-native run-android",
    "android:run4": "$ANDROID_HOME/tools/emulator -avd Nexus_S_API_22 & react-native run-android",
    "storybook": "storybook start -p 7007",
    "lint": "standard --verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.js$' | xargs standard | snazzy",
    "fixcode": "standard --fix",
    "git-hook": "npm run lint -s && npm run test -s",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/datetimepicker": "^2.1.0",
    "apisauce": "^0.14.2",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.2",
    "apollo-link": "^1.2.11",
    "apollo-link-error": "^1.1.10",
    "apollo-link-http": "^1.5.14",
    "apollo-upload-client": "^10.0.1",
    "convert-units": "^2.3.4",
    "format-json": "^1.0.3",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "identity-obj-proxy": "^3.0.0",
    "immutability-helper": "^3.0.0",
    "jetifier": "^1.6.3",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "moment-hijri": "^2.1.1",
    "prop-types": "^15.6.1",
    "querystringify": "1.0.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-native": "^0.59.5",
    "react-native-animatable": "^1.3.2",
    "react-native-calendars": "^1.188.0",
    "react-native-config": "^0.10.0",
    "react-native-datepicker": "^1.7.2",
    "react-native-dropdownalert": "^3.10.0",
    "react-native-easy-grid": "^0.2.1",
    "react-native-fast-image": "^7.0.2",
    "react-native-fb-collage": "^1.0.2",
    "react-native-flex-image": "^1.3.0",
    "react-native-geolocation-service": "^2.0.0",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-gps-state": "^2.0.3",
    "react-native-grid-component": "^2.0.0",
    "react-native-htmlview": "^0.13.0",
    "react-native-image-crop-picker": "^0.24.1",
    "react-native-image-resizer": "^1.1.0",
    "react-native-image-zoom-viewer": "^2.2.26",
    "react-native-indicators": "^0.13.0",
    "react-native-js-bottom-sheet": "^1.1.0",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-linear-gradient": "^2.5.4",
    "react-native-maps": "0.25.0",
    "react-native-material-textfield": "^0.12.0",
    "react-native-modal": "^11.4.0",
    "react-native-modalbox": "^1.7.1",
    "react-native-onesignal": "^3.2.12",
    "react-native-pdf": "^5.1.5",
    "react-native-permissions": "^2.0.0-rc.1",
    "react-native-picker-select": "^6.2.0",
    "react-native-progress": "^3.6.0",
    "react-native-scroll-into-view": "^1.0.3",
    "react-native-sentry": "^0.43.2",
    "react-native-splash-screen": "^3.2.0",
    "react-native-step-indicator": "0.0.11",
    "react-native-super-grid": "^3.0.4",
    "react-native-svg": "^9.5.1",
    "react-native-text-input-mask": "^2.0.0",
    "react-native-timeline-listview": "^0.2.3",
    "react-native-vector-icons": "^6.5.0",
    "react-native-view-overflow": "0.0.4",
    "react-navigation": "3.0.0",
    "react-navigation-redux-helpers": "^2.0.6",
    "react-redux": "^5.0.6",
    "recyclerlistview": "^1.3.4",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0",
    "reduxsauce": "0.7.0",
    "reselect": "^4.0.0",
    "rn-fetch-blob": "^0.11.2",
    "seamless-immutable": "^7.1.4",
    "throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.4.0",
    "@storybook/addons": "^3.4.0",
    "@storybook/channels": "^3.4.0",
    "@storybook/react-native": "^3.4.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "24.1.0",
    "babel-plugin-ignite-ignore-reactotron": "^0.3.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^3.0.2",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "ignite-ir-boilerplate-andross": "^2.4.1",
    "ignite-redux-persist": "^1.1.2",
    "ignite-standard": "^1.0.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "mockery": "^2.1.0",
    "react-dom": "16.3.0",
    "react-test-renderer": "16.6.1",
    "reactotron-react-native": "^2.1.0",
    "reactotron-redux": "^2.1.0",
    "reactotron-redux-saga": "^2.1.0",
    "snazzy": "^8.0.0",
    "standard": "10.0.2"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "./App/Assets/Fonts"
    ]
  },
  "config": {},
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "test",
      "jest",
      "expect",
      "fetch",
      "navigator",
      "__DEV__",
      "XMLHttpRequest",
      "FormData",
      "React$Element"
    ]
  }
}
